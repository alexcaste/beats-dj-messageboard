<div class="row">
  <div class="col-md-4">
    <h2 class="fontsize fancyfont ">It's All About the Music</h2>
    <hr>
    <div class="row well">
      <ul class="list-unstyled">

        {{#each question in model}}

        <li>
          <span class="fancyfont">{{#link-to 'question' question.id}}{{question.text}}{{/link-to}}</span>
        </li>
        <br>
        {{/each}}
      </ul>


      <p><button class="btn btn-default btn-lg btn-block" {{action 'showModal' 'question-modal' model}}>Got a Question?</button>
      </p>
    </div>
  </div>




<div class='col-md-8'>
    {{outlet}}
</div>
</div>
